  },
  "devDependencies": {
    "chalk": "4.1.1",
    "dotenv-webpack": "7.0.3",
    "esbuild-loader": "2.13.1",
    "fork-ts-checker-webpack-plugin": "6.2.10",
    "jest": "27.0.4",
    "jest-transform-graphql": "2.1.0",
    "nodemon-webpack-plugin": "4.5.2",
